<h1>Add Collaborator</h1>

<%= form_for [@wiki, @collaborator] do |f| %>

  <div class="form-group">
    <%= f.label :user %>
    <%= f.select :user_id, options_from_collection_for_select((User.all - @wiki.collaborating_users), "id", "email") %>
  </div>

  <div class="form-group">
     <%= f.submit "Save", class: 'btn btn-success' %>
  </div>
<% end %>
